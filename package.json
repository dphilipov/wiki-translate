{
  "name": "shadow-scrape",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "tsx src/cli/index.ts",
    "type-check": "tsc --noEmit",
    "server": "tsx server/index.ts",
    "client": "cd client && yarn dev",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "build": "cd client && yarn build",
    "install-all": "yarn install && cd client && yarn install"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "deepl-node": "^1.19.0",
    "dotenv": "^16.0.3",
    "filenamify": "^5.1.1",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "concurrently": "^8.2.2"
  }
}
